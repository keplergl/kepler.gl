{
  "name": "kepler.gl-website",
  "description": "Website for kepler.gl",
  "license": "MIT",
  "author": "Shan He <shan@uber.com>",
  "main": "dist/index.js",
  "version": "0.0.1",
  "scripts": {
    "dev": "node esbuild.config.mjs --start",
    "esbuild": "yarn build-clean && yarn build-static && node esbuild.config.mjs --build",
    "start": "webpack serve --mode development --env prod --static-directory ./src/static --progress --open",
    "start-prod": "webpack serve --mode production --env prod --static-directory ./src/static --progress --open",
    "build-clean": "rm -rf ./dist && mkdir dist",
    "build-static": "cp -r ./src/static/* dist",
    "build-script": "NODE_OPTIONS=--openssl-legacy-provider webpack --mode production --env prod",
    "build": "yarn build-clean && yarn build-static && yarn esbuild",
    "lint": "eslint src"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "global": "^4.3.2",
    "lodash": "4.17.21",
    "prop-types": "^15.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-palm": "^3.1.2",
    "react-redux": "^8.0.5",
    "react-router": "3.2.6",
    "react-router-redux": "^4.0.8",
    "react-share": "^2.1.1",
    "react-swipeable-views": "^0.12.13",
    "react-waypoint": "^9.0.1",
    "redux": "^4.2.1",
    "redux-actions": "^1.2.0",
    "redux-thunk": "^1.0.0",
    "styled-components": "6.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/plugin-transform-class-properties": "^7.12.1",
    "@babel/plugin-transform-export-namespace-from": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-optional-chaining": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.16.7",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "css-loader": "^6.8.1",
    "esbuild": "^0.23.1",
    "esbuild-plugin-replace": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-uber-es2015": "^3.1.2",
    "eslint-config-uber-jsx": "^3.3.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^1.1.11",
    "prettier": "1.19.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^3.3.3",
    "url-loader": "^0.5.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.15.1",
    "webpack-hot-middleware": "^2.25.4"
  },
  "peerDependencies": {
    "react": "18.x",
    "react-dom": "18.x"
  },
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "18.18.2",
    "yarn": "4.4.0"
  },
  "packageManager": "yarn@4.4.0"
}
