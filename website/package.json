{
  "name": "kepler.gl-website",
  "description": "Website for kepler.gl",
  "license": "MIT",
  "author": "Shan He <shan@uber.com>",
  "main": "dist/index.js",
  "version": "0.0.1",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --env.prod --content-base ./src/static --progress --open",
    "start-prod": "webpack-dev-server -p --env.prod --content-base ./src/static --progress --open",
    "build-clean": "rm -rf ./dist && mkdir dist",
    "build-static": "cp -r ./src/static/* dist",
    "build-script": "NODE_OPTIONS=--openssl-legacy-provider webpack -p --env.prod",
    "build": "npm run build-clean && npm run build-static && npm run build-script",
    "lint": "eslint src"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "global": "^4.3.2",
    "lodash.get": "^4.4.2",
    "prop-types": "^15.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-palm": "^3.1.2",
    "react-redux": "^8.0.5",
    "react-router": "3.2.5",
    "react-router-redux": "^4.0.8",
    "react-share": "^2.1.1",
    "react-swipeable-views": "^0.12.13",
    "react-waypoint": "^9.0.1",
    "redux": "^4.2.1",
    "redux-actions": "^1.2.0",
    "redux-thunk": "^1.0.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.16.7",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-uber-es2015": "^3.1.2",
    "eslint-config-uber-jsx": "^3.3.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^1.1.11",
    "prettier": "1.19.1",
    "source-map-loader": "^0.2.1",
    "url-loader": "^0.5.9",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3"
  },
  "peerDependencies": {
    "react": "18.x",
    "react-dom": "18.x"
  }
}
